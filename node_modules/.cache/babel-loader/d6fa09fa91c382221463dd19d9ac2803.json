AAA,CAAA,SAAA,IAAA,QAAA,CAAA,SAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAE3C,GAFT,EAAiE,iBAAjE,CAGK,CAxCP,C,qCA0CE,CA3DN,C","sourcesContent":["<template>\n  <div class=\"card\">\n    <div class=\"card-header\">\n      <h2 class=\"mb-0\">\n        <button\n          class=\"btn btn-link btn-block text-left d-flex\"\n          type=\"button\"\n          @click=\"toggleExpand\"\n        >\n          <div class=\"mr-2\">Create New Window</div>\n          <div class=\"expand-button ml-auto \">\n            {{ isExpanded ? \"&#9660;\" : \"&#9658;\" }}\n          </div>\n        </button>\n      </h2>\n    </div>\n\n    <template class=\"collapse show\" v-if=\"isExpanded\">\n      <form class=\"p-4\">\n        <div class=\"form-group\">\n          <label for=\"window_name\">Window Name: </label>\n          <input\n            v-model=\"window_name\"\n            type=\"text\"\n            class=\"form-control\"\n            name=\"window_name\"\n            id=\"window_name\"\n            placeholder=\"window\"\n          />\n        </div>\n        <div class=\"form-group\">\n          <label for=\"window_status\">Window Status: </label>\n          <select\n            v-model=\"window_status\"\n            class=\"form-control\"\n            id=\"window_status\"\n            name=\"window_status\"\n          >\n            <option value=\"CLOSED\">Closed</option>\n            <option value=\"OPEN\">Open</option>\n          </select>\n        </div>\n        <div class=\"form-group\">\n          <label for=\"window_room\">Window Room: </label>\n          <select\n            class=\"form-control\"\n            v-model=\"window_room\"\n            id=\"window_room\"\n            name=\"window_room\"\n          >\n            <option v-for=\"room in rooms\" v-bind:value=\"room\">\n              {{ room.name }}\n            </option>\n          </select>\n        </div>\n        <button type=\"button\" class=\"btn btn-primary\" @click=\"addWindow\">\n          Create Window\n        </button>\n      </form>\n    </template>\n  </div>\n</template>\n\n\n<script>\nimport axios from \"axios\";\nimport { API_HOST } from \"../config\";\n\nexport default {\n  name: \"WindowNew\",\n  data: function () {\n    return {\n      isExpanded: false,\n      window_name: \"\",\n      window_status: \"CLOSED\",\n      window_room: \"\",\n      rooms: [],\n    };\n  },\n  created: async function () {\n    let response = await axios.get(`${API_HOST}/room`);\n    let rooms = response.data;\n    this.rooms = rooms;\n    this.window_room = rooms[0];\n  },\n  methods: {\n    toggleExpand() {\n      this.isExpanded = !this.isExpanded;\n    },\n    async addWindow() {\n      let newWindow = {\n        name: this.window_name,\n        room: this.window_room,\n        windowStatus: this.window_status,\n      };\n      let headers = {\n        \"Content-Type\": \"application/json\",\n      };\n      let response = await axios.post(`${API_HOST}/window`, newWindow, {\n        headers,\n      });\n      this.$emit(\"window-added\", response.data);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.open-status {\n  .icon {\n    position: relative;\n  }\n\n  &.open {\n    color: #198754;\n    .icon {\n      font-size: 12px;\n      top: -2px;\n    }\n  }\n\n  &.closed {\n    color: #dc3545;\n  }\n}\n\n.window {\n  .top-row {\n    cursor: pointer;\n  }\n}\n</style>\n"],"sourceRoot":""}]}