,WAAA,EAQM,C,gBAPJ,mBAAA,CAME,OANF,EAME;AALA,aAAM,cAKN;AAJA,IAAA,IAAI,EAAC,QAIL;;aAHS,IAAA,CAAA,wB,GAAwB,M;MAGjC;AAFA,IAAA,EAAE,EAAC,0BAEH;AADA,IAAA,IAAI,EAAC;AACL,GANF,E,IAAA,E;;AAAA,G,iBAGW,IAAA,CAAA,wB,GAIP,CARN,CASI,CAXN,CAuCK,EA3BL,mBAAA,CAWM,KAXN,EAAA,WAAA,EAWM,CAVJ,WAUI,EATJ,mBAAA,CAQM,KARN,EAAA,WAAA,EAQM,C,gBAPJ,mBAAA,CAME,OANF,EAME;AALA,aAAM,cAKN;AAJA,IAAA,IAAI,EAAC,QAIL;;aAHS,IAAA,CAAA,uB,GAAuB,M;MAGhC;AAFA,IAAA,EAAE,EAAC,yBAEH;AADA,IAAA,IAAI,EAAC;AACL,GANF,E,IAAA,E;;AAAA,G,iBAGW,IAAA,CAAA,uB,GAIP,CARN,CASI,CAXN,CA2BK,EAfL,mBAAA,CAWM,KAXN,EAAA,WAAA,EAWM,CAVJ,WAUI,EATJ,mBAAA,CAQM,KARN,EAAA,WAAA,EAQM,C,gBAPJ,mBAAA,CAME,OANF,EAME;AALA,aAAM,cAKN;AAJA,IAAA,IAAI,EAAC,QAIL;;aAHS,IAAA,CAAA,U,GAAU,M;MAGnB;AAFA,IAAA,EAAE,EAAC,YAEH;AADA,IAAA,IAAI,EAAC;AACL,GANF,E,IAAA,E;;AAAA,G,iBAGW,IAAA,CAAA,U,GAIP,CARN,CASI,CAXN,CAeK,EAHL,mBAAA,CAES,QAFT,EAES;AAFD,IAAA,IAAI,EAAC,QAEJ;AAFa,aAAM,iBAEnB;AAFsC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,OAAA,IAAA,QAAA,CAAA,OAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAE3C,GAFT,EAA+D,eAA/D,CAGK,CArEP,C,qCAuEE,CAxFN,C","sourcesContent":["<template>\n  <div class=\"card\">\n    <div class=\"card-header\">\n      <h2 class=\"mb-0\">\n        <button\n          class=\"btn btn-block text-left d-flex\"\n          type=\"button\"\n          @click=\"toggleExpand\"\n        >\n          <div class=\"mr-2 text-primary\">Create New Room</div>\n          <div class=\"expand-button ml-auto text-primary\">\n            {{ isExpanded ? \"&#9660;\" : \"&#9658;\" }}\n          </div>\n        </button>\n      </h2>\n    </div>\n\n    <template class=\"collapse show\" v-if=\"isExpanded\">\n      <form class=\"p-4\">\n        <div class=\"form-group row\">\n          <label for=\"room_name\" class=\"col-sm-4 col-form-label\">Room Name: </label>\n          <div class=\"col-sm-8\">\n            <input\n              class=\"form-control\"\n              v-model=\"room_name\"\n              type=\"text\"\n              id=\"room_name\"\n              name=\"room_name\"\n              placeholder=\"Hall\"\n            />\n          </div>\n        </div>\n\n        <div class=\"form-group row\">\n          <label for=\"room_building\" class=\"col-sm-4 col-form-label\">Room building:</label>\n          <div class=\"col-sm-8\">\n            <select\n              class=\"form-control\"\n              v-model=\"room_building\"\n              id=\"room_building\"\n              name=\"room_building\"\n            >\n              <option v-for=\"building in buildings\" v-bind:value=\"building\">\n                {{ building.name }}\n              </option>\n            </select>\n          </div>\n        </div>\n        <div class=\"form-group row\">\n          <label for=\"room_current_temperature\" class=\"col-sm-4 col-form-label\">Room Current Temperature:</label>\n          <div class=\"col-sm-8\">\n            <input\n              class=\"form-control\"\n              type=\"number\"\n              v-model=\"room_current_temperature\"\n              id=\"room_current_temperature\"\n              name=\"room_current_temperature\"\n            />\n          </div>\n        </div>\n        <div class=\"form-group row\">\n          <label for=\"room_target_temperature\" class=\"col-sm-4 col-form-label\">Room Target Temperature:</label>\n          <div class=\"col-sm-8\">\n            <input\n              class=\"form-control\"\n              type=\"number\"\n              v-model=\"room_target_temperature\"\n              id=\"room_target_temperature\"\n              name=\"room_target_temperature\"\n            />\n          </div>\n        </div>\n        <div class=\"form-group row\">\n          <label for=\"room_floor\" class=\"col-sm-4 col-form-label\">Room Floor: </label>\n          <div class=\"col-sm-8\">\n            <input\n              class=\"form-control\"\n              type=\"number\"\n              v-model=\"room_floor\"\n              id=\"room_floor\"\n              name=\"room_floor\"\n            />\n          </div>\n        </div>\n        <button type=\"button\" class=\"btn btn-primary\" @click=\"addRoom\">\n          Create Room\n        </button>\n      </form>\n    </template>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { API_HOST } from \"../config\";\n\nexport default {\n  name: \"RoomNew\",\n  data: function () {\n    return {\n      isExpanded: false,\n      room_name: \"\",\n      room_building: \"\",\n      room_current_temperature: \"\",\n      room_target_temperature: \"\",\n      room_floor: \"\",\n      buildings: [],\n    };\n  },\n  created: async function () {\n    let response = await axios.get(`${API_HOST}/api/buildings`);\n    let buildings = response.data;\n    this.buildings = buildings;\n    this.room_building = buildings[0];\n  },\n  methods: {\n    toggleExpand() {\n      this.isExpanded = !this.isExpanded;\n    },\n    async addRoom() {\n      let newRoom = {\n        name: this.room_name,\n        buildingId: this.room_building.id,\n        currentTemperature: this.room_current_temperature,\n        targetTemperature: this.room_target_temperature,\n        floor: this.room_floor,\n      };\n      let headers = {\n        \"Content-Type\": \"application/json\",\n      };\n      let response = await axios.post(`${API_HOST}/api/rooms/`, newRoom, {\n        headers,\n      });\n      this.$emit(\"room-added\", response.data);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.open-status {\n  .icon {\n    position: relative;\n  }\n\n  &.open {\n    color: #198754;\n    .icon {\n      font-size: 12px;\n      top: -3px;\n    }\n  }\n\n  &.closed {\n    color: #dc3545;\n  }\n}\n\n.room {\n  .top-row {\n    cursor: pointer;\n  }\n}\n</style>\n"],"sourceRoot":""}]}